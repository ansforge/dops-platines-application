			<append value="ENGINE=INNODB DEFAULT CHARSET=UTF8" />
		</modifySql>
	</changeSet>
	<changeSet author="aboittiaux"
		id="ajoutClePrimaireDonneesFonctionnellesPlatines">
		<addPrimaryKey columnNames="id"
			tableName="donnees_fonctionnelles" />
	</changeSet>

