	<acl_entry id="34" acl_object_identity="6" ace_order="0"
		sid="7" mask="1" granting="1" audit_success="1" audit_failure="1" />
	<acl_entry id="35" acl_object_identity="6" ace_order="1"
		sid="7" mask="2" granting="1" audit_success="1" audit_failure="1" />

